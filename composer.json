{
    "name": "beriyack/jamf-api-client",
    "description": "Un client PHP pour interagir avec l'API de Jamf, construit sur beriyack/api-client.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Beriyack\\Jamf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beriyack\\Jamf\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Beriyack",
            "homepage": "https://github.com/Beriyack"
        }
    ],
    "keywords": [
        "jamf",
        "mdm",
        "api",
        "sdk",
        "php",
        "zuludesk",
        "apple",
        "device management"
    ],
    "require": {
        "php": ">=8.1",
        "beriyack/api-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
